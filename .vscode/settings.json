{
  "workbench.colorCustomizations": {
    "activityBar.background": "#999694",
    "activityBar.foreground": "#000000",
    "statusBar.background": "#999694"
  },
  "workbench.colorTheme": "Default Light+",
  "files.exclude": {},
  "search.exclude": {
    "bin": true,
    "local_release": true,
    "node_modules": true,
    ".build": true,
    ".vscode": true,
    "browser": true,
    "dist": true,
    "package-lock.json": true,
    ".firedev": true,
    ".taon": true,
    ".tnp": true,
    "**/bin": true,
    "**/local_release": true,
    "**/node_modules": true,
    "**/.build": true,
    "**/.vscode": true,
    "**/browser": true,
    "**/dist": true,
    "**/package-lock.json": true,
    "**/.firedev": true,
    "**/.taon": true,
    "**/.tnp": true,
    "projects": true
  },
  "json.schemas": [
    {
      "fileMatch": [
        "/taon.jsonc"
      ],
      "url": "./taon-config-container.schema.json"
    }
  ],
  "window.title": "dfilipiak/npm/ui-libs  [ ${activeEditorShort} ]",
  "search.useIgnoreFiles": false,
  "search.include": [
    "**/src/**"
  ]
}